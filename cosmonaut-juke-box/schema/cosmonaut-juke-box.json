{
  "contract_name": "cosmonaut-juke-box",
  "contract_version": "0.1.0",
  "idl_version": "1.0.0",
  "instantiate": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "InstantiateMsg",
    "type": "object",
    "additionalProperties": false
  },
  "execute": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ExecuteMsg",
    "oneOf": [
      {
        "type": "string",
        "enum": [
          "reset_leds"
        ]
      },
      {
        "type": "object",
        "required": [
          "change_leds"
        ],
        "properties": {
          "change_leds": {
            "type": "object",
            "required": [
              "blue",
              "green",
              "red"
            ],
            "properties": {
              "blue": {
                "type": "integer",
                "format": "uint8",
                "minimum": 0.0
              },
              "green": {
                "type": "integer",
                "format": "uint8",
                "minimum": 0.0
              },
              "red": {
                "type": "integer",
                "format": "uint8",
                "minimum": 0.0
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "play_sound"
        ],
        "properties": {
          "play_sound": {
            "type": "object",
            "required": [
              "index"
            ],
            "properties": {
              "index": {
                "type": "integer",
                "format": "uint",
                "minimum": 0.0
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      }
    ]
  },
  "query": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "QueryMsg",
    "oneOf": [
      {
        "type": "object",
        "required": [
          "get_leds"
        ],
        "properties": {
          "get_leds": {
            "type": "object",
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      }
    ]
  },
  "migrate": null,
  "sudo": null,
  "responses": {
    "get_l_e_ds": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "led_response",
      "type": "object",
      "required": [
        "blue",
        "green",
        "red"
      ],
      "properties": {
        "blue": {
          "type": "integer",
          "format": "uint8",
          "minimum": 0.0
        },
        "green": {
          "type": "integer",
          "format": "uint8",
          "minimum": 0.0
        },
        "red": {
          "type": "integer",
          "format": "uint8",
          "minimum": 0.0
        }
      },
      "additionalProperties": false
    }
  }
}
